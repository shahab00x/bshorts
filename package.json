{
  "name": "bastyon-miniapp-vue-template",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.1.1",
  "description": "A Vue 3 template project demonstrating how to interact with the Bastyon platform using the Bastyon SDK.",
  "author": "Pocketnet Team",
  "license": "Apache-2.0",
  "scripts": {
    "build": "vite build",
    "dev": "vite --port 3333 --host",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks",
    "dev:debug": "cross-env VITE_SDK_DEBUG=true vite --host --port 3333"
  },
  "dependencies": {
    "@vueuse/core": "^10.9.0",
    "bitcoinjs-lib": "^5.2.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "events": "^3.3.0",
    "hls.js": "^1.5.8",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.18.1",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@iconify-json/carbon": "^1.2.2",
    "@rollup/plugin-inject": "^5.0.5",
    "@types/bastyon-sdk": "github:DaniilKimlb/types-bastyon-sdk",
    "@types/node": "^20.12.12",
    "@unocss/eslint-config": "^0.60.2",
    "@unocss/eslint-plugin": "^0.60.2",
    "@unocss/reset": "^0.60.2",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue-macros/volar": "^0.19.1",
    "@vue/test-utils": "^2.4.6",
    "cross-env": "^10.0.0",
    "eslint": "^9.3.0",
    "eslint-plugin-format": "^0.1.1",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "pnpm": "^9.1.1",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.13.8",
    "typescript": "^5.4.5",
    "unocss": "^0.60.2",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.27.0",
    "unplugin-vue-macros": "^2.9.2",
    "unplugin-vue-router": "^0.8.6",
    "vite": "^5.2.11",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.19"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
